{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "preview-testflight": {
      "distribution": "store",
      "autoIncrement": true,
      "channel": "preview",
      "extends": "preview"
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true
    }
  },
  "submit": {
    "preview-testflight": {},
    "production": {}
  }
}